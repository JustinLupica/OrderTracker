<h1><%= @order.pickup_date.strftime("%m/%d/%y") %></h1>
   <%= link_to @order.customer.first_name + " " + @order.customer.last_name, customer_path(@order.customer) %> <br> <br>
<% if @order.products.nil? %>
   <label>No products associated with this order..</label>
<% else %>
   <% @order.products.each do |product| %>
   <%= product.category %> <br>
   <%= product.size %> <br>
   <%= product.flavor %> <br>
<% end %>
<%= link_to "Edit", edit_order_path %>
<% end %>
